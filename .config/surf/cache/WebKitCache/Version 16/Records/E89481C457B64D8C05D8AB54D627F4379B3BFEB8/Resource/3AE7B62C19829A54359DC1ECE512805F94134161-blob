  display: inline-block;
  width: 24px;
  height: 24px;
  margin: -3px 4px 0 0;
  vertical-align: middle;
  cursor: pointer;
  border-radius: 3px;

  background: transparent url("/img/sprite.png") repeat-x scroll -333px -165px;
  position: relative;
}

