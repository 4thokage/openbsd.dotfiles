  cursor: pointer;
  height: 28px;



}

.ui_checkbox input[type="checkbox"]:disabled + label,
.ui_checkbox input[type="checkbox"]:disabled + label i {
  filter:progid:DXImageTransform.Microsoft.Alpha(opacity=65);
  -moz-opacity: 0.65;
  -khtml-opacity: 0.65;
  opacity: 0.65;
  cursor: default;
}

