        (function(w, d){
            var b = d.getElementsByTagName('body')[0];
            var s = d.createElement("script");
            var hasIO = "IntersectionObserver" in w;
            s.async = true;
            w.lazyLoadOptions = {
                elements_selector: '.lazy'
            };
            s.src = hasIO ? "/js/vendor/lazyload.iife.js?v=@@wopt-deployment-version" : "https://cdn.jsdelivr.net/npm/vanilla-lazyload@8.17.0/dist/lazyload.min.js";
