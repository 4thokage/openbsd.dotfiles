<script>
Ecomisi.module("correcao-legal", [
  "vendor",
  function() {
    function toolTippers() {
      var doc = $(document);

      doc.find(".w-card__description br").remove();

      for (
        var i = 0;
        i <
        doc.find(/*.swiper-slide*/ ".w-card-promotions .w-card-wrapper h3")
          .length;
        i++
      ) {
        $(
          doc.find(/*.swiper-slide*/ ".w-card-promotions .w-card-wrapper h3")[i]
        ).append(
          ' <span data-tooltip aria-haspopup="true" class="has-tip" data-position="bottom" data-alignment="center" title="' +
            $(
              doc.find(
                /*.swiper-slide*/ ".w-card-promotions footer "
              )[i]
            ).text() +
            '" data-events="resize"><i class="w-icon-info-outline icon-v_align"></i></span>'
        );
      }

      $(doc).foundation();

      $(".w-card-promotions__link").on("click", function(e) {
        e.preventDefault();
        if (
          !$(e.target).hasClass("icon-v_align") &&
          !$(e.target).hasClass("has-tip")
        ) {
          window.location = e.currentTarget.href;
        } else {
          $(document).foundation();
        }
      });
    }
    function timeamos(){
      setTimeout(function() {
        if($(/*.swiper-slide*/ ".w-card-promotions footer").length){
          toolTippers()
        }else{
          timeamos()
        }
      }, 2000);
    }
    timeamos();
  }
]);


</script>


