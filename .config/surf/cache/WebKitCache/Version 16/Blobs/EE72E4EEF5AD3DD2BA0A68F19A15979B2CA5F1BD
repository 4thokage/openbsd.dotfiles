                       data-toggle="collapse"
                       title="Report">
                        <i class="icon-flag"></i>
                    </a>
                    
                                    </div> <!-- /.more-options -->
            </div> <!-- /.post-actions -->

            
            <div class="panel-group panel-blank" id="post-actions-353188">
                
            <div class="panel panel-default">
                <div
                    id="abuse-report-panel-353188"
                    data-role="abuse-report-panel"
                    data-form-url="https://www.steadyhealth.com/abuse-reports/send?contentType=11&contentId=353188&contentName=post"
                    class="panel-collapse collapse"
                    style="height: auto;"
                >
                    <div class="panel-body">
                        <p class="text-center"><span class="loading"><i class="loader loader-md"></i>Loading...</span></p>
