                                    var RELATIVE_TIME_TIMER = setTimeout(function() {
                                        $("time.relative-time").each(function() {
                                            var $this = $(this);
                                            $this.html(moment($this.attr("datetime")).fromNow());
                                        });
                                        RELATIVE_TIME_TIMER = setTimeout(arguments.callee, 60*1000);
                                    }, 100);
                                }
