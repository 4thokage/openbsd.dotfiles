  this.handleSale2xClick = function () {
    var $links = $('.video-group.active a, .bcdb_model_group_active a')
      .filter(function (i, el) {
        return !$(el).find('.sale2x').length; // filter hovered thumbs
      });
    var link = $links[Math.floor(Math.random() * $links.length)];
    if (link) {
      window.open(link.getAttribute('href'))
      return false;
    }
  }

  this.onCreate = function () {
    if ($('html[lang]').length) {
      $('body').addClass('lang_' + $('html').attr('lang'));
    }
    $(document).bind('sale2x:update', this.afterUpdate.bind(this));
    this.process();
    this.afterUpdate();
    $('.sale2x').bind('click', this.handleSale2xClick);
  };

  $('img.thumb').one('load', function () {
    if (!self.created) {
      var img = this;
      var waitThumbCounter = 0
      var waitThumb = setInterval(function () {
        if (self.created || !$(img).is(':visible')) {
          clearInterval(waitThumb);
          return;
        }
        waitThumbCounter++;
        var modelimage = $(img).closest('.modelimage')[0];
        if (modelimage) {
          self.thumbWidth = modelimage.getBoundingClientRect().width;
          self.thumbHeight = modelimage.getBoundingClientRect().height;
        }
        var item = $(img).closest('.bcdb_model_item')[0];
        if (item) {
          self.itemHeight = item.getBoundingClientRect().height;
        }
        if (waitThumbCounter > 10 || self.thumbHeight > 10 || !modelimage) {
          clearInterval(waitThumb);
          self.created = true;
          self.onCreate();
        }
      }, 100);
    }
  })
}

$(function () {
  window.Sale2x = new Sale2x();
});
  </script>
</head>
