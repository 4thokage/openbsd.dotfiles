function fitTextFontSize(selector, parent, options) {
  parent = parent ? parent : document.body;
  options = $.extend({ignoreHeight: false}, options);

  function forEach(array, callback) {
    for (var i = 0; i < array.length; i++) {
      callback.call(null, array[i]); // passes back stuff we need
    }
  }

  function isFit(el) {
    return el.scrollWidth <= el.clientWidth && (el.scrollHeight <= el.clientHeight || options.ignoreHeight);
  }

  function dichotomy(min, max, el) {
    if (max - min < 0.5) {
      el.style.fontSize = min + 'px';
      return min;
    } else {
      var mid = (max + min) / 2;
      el.style.fontSize = mid + 'px';
      if (isFit(el)) {
        return dichotomy(mid, max, el);
      } else {
        return dichotomy(min, mid, el);
      }
    }
  }

  var commonMinFS = Infinity;
  var elements = parent.querySelectorAll(selector);
  forEach(elements, function (el) {
    el.style.fontSize = '';
    var fsize = options.initSize || parseInt(window.getComputedStyle(el)['font-size'], 10);
    commonMinFS = Math.min(commonMinFS, isFit(el) ? fsize : dichotomy(1, fsize, el));
  })
  if (options.syncSize) {
    forEach(elements, function (el) {
      el.style.fontSize = commonMinFS + 'px';
    })
  }
}

function Sale2x() {
  var self = this;
  this.params = JSON.parse('{"bannerNumber":"","lang":"pt","width":"300","height":"250","header":"0","footer":"footer_text_1","mname":"0","mlink":"0","mode":"mode1","slider":"0","fullscreen":"","msize":"auto","text_color":"#000000"}');
  this.translates = JSON.parse('{"tokens":"Tokens","2 times":"2 VEZES","cheaper!":"mais baratos!"}');

  this.onDone = function () {
  };

  this.afterUpdate = function () {
    var $sale2x = $('.sale2x');
    if ($sale2x.height() < 100) {
      $sale2x.addClass('__mini');
    }

    // fix full-visible coins for wide banners
    var $coin = $sale2x.find('.sale2x-bg');
    if ($coin.length) {
      if ($coin.width() / $coin.height() > 0.8) {
        $coin.css('min-width', $coin.height() * 1.063);
      }
    }
    fitTextFontSize('.sale2x > .sale2x-wrp > .sale2x-line');
  }

  this.addOverThumb = function (params, bannerParams) {
    params = $.extend({number: 2, forceUnderThumbText: false, mlinkTextOnly: false}, params);
    bannerParams = $.extend({}, {lines: 3}, bannerParams);
    var $anchor;
    if ($('.bcdb_model_group > .bcdb_row').length) {
      $anchor = $('.bcdb_row:nth-child(1) .bcdb_model_item:nth-child(' + params.number + ') .modelimage');
    } else {
      $anchor = $('.bcdb_model_item:nth-child(' + params.number + ') .modelimage');
    }
    $anchor.append($('<div style="position: absolute; width: ' + this.thumbWidth + 'px; height: ' + (this.thumbHeight + 1) + 'px; z-index: 100">')
      .append(this.getBanner(bannerParams)));
    var $link = $anchor.closest('a');
    var $item = $link.closest('.bcdb_model_item');

    function updateUnderThumbText() {
      function updateContainerHeight() {
        fitTextFontSize('[data-newtext]', document.body, {ignoreHeight: true});
        var $gcont = $link.closest('.bcdb_model_group_container');
        $gcont.css('height', Math.max($gcont.height(), $link.outerHeight()));
      }

      if (+self.params.mlink && +self.params.mname || (params.forceUnderThumbText && (+self.params.mlink || +self.params.mname))) {
        $link.find('.modelname, .modellink').remove();
        $link.append('<span class="modelname" data-newtext=1 style="overflow: visible; color: ' +
          self.params.text_color + '">' + self.translates['tokens'] + '</span><span class="modellink" data-newtext=1>' +
          self.translates['2 times'] + ' ' + self.translates['cheaper!'] + '</span>');
        if (params.mlinkTextOnly) {
          $link.find('.modellink').text(self.translates['tokens'] + ' ' +
            self.translates['2 times'] + ' ' + self.translates['cheaper!']);
        }
        if (params.forceUnderThumbText) {
          $link.find('img').one('load', updateContainerHeight);
          updateContainerHeight();
        }
      } else if (!!+self.params.mlink !== !!+self.params.mname) {
        $link.find('.modelname, .modellink').attr('data-newtext', 1).text(self.translates['tokens'] + ' ' +
          self.translates['2 times'] + ' ' + self.translates['cheaper!']);
        $link.find('img').one('load', updateContainerHeight);
        updateContainerHeight();
      }
      if (self.type !== 'fullscreen') {
        if (self.itemHeight > 10) {
          $('.bcdb_model_item').css('height', self.itemHeight + 0.1);
        }
        if ($item[0] && $item[0].scrollHeight > $item[0].offsetHeight && !params.forceUnderThumbText) {
          $link.find('.modelname, .modellink').css({'visibility': 'hidden'})
        }
      }
    }

    updateUnderThumbText();
    $(document).bind('sale2x:updateModels', updateUnderThumbText);
    return $anchor;
  }

  this.addUnderThumb = function (params, bannerParams) {
    params = $.extend({}, {height: 48}, params);
    bannerParams = $.extend({}, {lines: 2}, bannerParams);
    var $anchor = $('.bcdb_model_item .modelimage');
    $('.bcdb_footer').css({'padding-top': '2px', 'margin-top': 0});
    var height = Math.min(params.height, $('html').height() - $('.bcdb_header').outerHeight() - $('.bcdb_footer').outerHeight() - this.thumbHeight - 10);
    $anchor.after($('<div style="position: relative; width: ' + this.thumbWidth + 'px; height: ' + height +
      'px; z-index: 100; border: solid 1px #ffffff; box-sizing: border-box; margin-top: 4px">')
      .append(this.getBanner(bannerParams)));
    $anchor.each(function (index, el) {
      var $_anchor = $(el);
      var $link = $_anchor.closest('a');
      $('.bcdb_model_group_container').height($('html').height() - $('.bcdb_header').outerHeight() - $('.bcdb_footer').outerHeight());
      if (params.metaOver) {
        var $mName = $link.find('.modelname');
        var $mLink = $link.find('.modellink');
        var $both = $mName.add($mLink);
        if ($both.length) {
          $_anchor.append($both);
          $both.addClass('sale2x-meta-over-thumb').addClass($both.length % 2 === 0 ? '__half' : '');
        }
      }
    })
  };

  this.addInnerThumb = function (params, bannerParams) {
    params = $.extend({edge: 'sw', height: 48, maxWidth: 150, css: ''}, params);
    bannerParams = $.extend({}, {lines: 2}, bannerParams);
    var $anchor = $('.bcdb_model_item .modelimage');
    var height = 48;
    var pos = params.edge[0] === 'n' ? 'top: 0;' : 'bottom: 0;';
    pos += params.edge[1] === 'w' ? 'left: 0;' : 'right: 0;';
    $anchor.append($('<div style="position: absolute; max-width: ' + params.maxWidth + 'px; width: ' + this.thumbWidth + 'px; height: ' + params.height +
      'px; z-index: 100; border: solid 1px #ffffff; box-sizing: border-box; ' + pos + ';' + params.css + '">')
      .append(this.getBanner(bannerParams)));
  };

  /**
   * Shows
   * @param params
   * @param bannerParams
   */
  this.addOverTwoThumb = function (params, bannerParams) {
    params = $.extend({number: 2}, params);
    bannerParams = $.extend({}, {lines: 3}, bannerParams);
    var $anchor1 = $('.bcdb_model_item:nth-child(' + params.number + ') .modelimage');
    var $anchor2 = $('.bcdb_model_item:nth-child(' + (params.number + 1) + ') .modelimage');
    if (!$anchor1.length || !$anchor2.length) {
      return;
    }
    var b1 = $anchor1[0].getBoundingClientRect();
    var b2 = $anchor2[0].getBoundingClientRect();
    var height = this.params.height - b1.top * 2;
    var width = b2.left + b2.width - b1.left;

    $anchor1.closest('a').prepend($('<div style="position: absolute; width: ' + width + 'px; height: ' + height + 'px; z-index: 100">')
      .append(this.getBanner(bannerParams))).css({width: width + 'px', height: height + 'px'});
    $anchor1.closest('.bcdb_model_group_container').css('height', height + 'px');
  };

  this.addAboveFooter = function (params, bannerParams) {
    params = $.extend({css: '', height: 100}, params);
    bannerParams = $.extend({}, {lines: 3}, bannerParams);
    var $banner = $('<div style="height: ' + params.height + 'px; ' + params.css + '">')
      .append(this.getBanner(bannerParams))
    var $footer = $('.bcdb_footer');
    if ($footer.length) {
      $footer.before($banner);
    } else {
      $('.bcdb').append($banner);
    }
  }

  this.addRelativeSelector = function (params, bannerParams) {
    params = $.extend({css: ''}, params);
    bannerParams = $.extend({}, bannerParams);

    var $anchor = $(params.anchor);
    $anchor.append($('<div style="position: absolute; z-index: 100; ' + params.css + '">')
      .append(this.getBanner(bannerParams)));
  }

  this.addRelativeLink = function (params, bannerParams) {
    params = $.extend({anchor: '.bcdb_model_item > a'}, params);
    bannerParams = $.extend({className: 'bordered'}, {lines: 3}, bannerParams);
    this.addRelativeSelector(params, bannerParams);
  }

  this.addRelativeGroup = function (params, bannerParams) {
    params = $.extend({anchor: '.bcdb_model_item'}, params);
    bannerParams = $.extend({className: 'bordered'}, {lines: 3}, bannerParams);
    this.addRelativeSelector(params, bannerParams);
  }

  this.addRelativeBody = function (params, bannerParams) {
    params = $.extend({anchor: 'body'}, params);
    bannerParams = $.extend({className: 'bordered'}, {lines: 3}, bannerParams);
    this.addRelativeSelector(params, bannerParams);
  }


  this.addOverThumbTargeted = function (params, bannerParams) {
    params = $.extend({number: 2}, params);
    bannerParams = $.extend({}, {lines: 3}, bannerParams);
    var $anchor = $('.video-group .video:nth-child(' + params.number + ') .slide-container');
    $anchor.append($('<div style="position: absolute; width: ' + $anchor.width() + 'px; height: ' + $anchor.height() + 'px; top: 0; z-index: 100">')
      .append(this.getBanner(bannerParams)));
    $anchor.closest('a').find('.bage').remove();
  };

  this.addFlipTargeted = function (params, bannerParams) {
    params = $.extend({selector: '.logo', css: ''}, params);
    bannerParams = $.extend({className: 'bordered'}, {lines: 2}, bannerParams);
    var $anchors = $(params.selector);
    if (!$anchors.length) {
      return
    }
    $anchors.each(function (i, el) {
      var $anchor = $(el);
      var rect = $anchor[0].getBoundingClientRect();
      var $flip = $('<div class="sale2x-flip-container" style="position: fixed; top: ' + rect.top + 'px; left: ' + rect.left + 'px; width: ' + rect.width + 'px; height: ' + rect.height + 'px; z-index: 100">')
      var $flipper = $('<div class="sale2x-flipper">').appendTo($flip);
      $anchor.after($flip).css({top: 0, left: 0, right: 0, bottom: 0, width: '100%', height: '100%'});
      $flipper.append($('<div class="sale2x-flip-side sale2x-flip-side-a">').append($anchor));
      $flipper.append($('<div class="sale2x-flip-side sale2x-flip-side-b" style="' + params.css + '">').append(self.getBanner(bannerParams)));
      setInterval(function () {
        $flip.toggleClass('__flipped');
      }, 2000);
    })
  }

  this.replaceText = function (params) {
    params = $.extend({selector: '.text', bgCss: '', textCss: '', shiftLogo: false}, params);
    setTimeout(function () {
      var $text = $(params.selector);
      if (!$text.length) {
        return
      }
      var tb = $text[0].getBoundingClientRect();
      var coinWidth = tb.height * 0.6;
      $text.attr('style', params.textCss).css({
        'opacity': 1,
        'font-size': '20px',
        'margin-left': coinWidth + 'px',
        'text-transform': 'uppercase'
      });
      $text.find('.mgctxt-wrapper > *').unwrap();
      var $welcome = $text.find('.welcome');
      $welcome.css('padding-left', parseInt($welcome.css('padding-left'), 10) - coinWidth);
      var $main = $text.find('.main');
      $main.html(self.translates['tokens'] + ' ' +
        self.translates['2 times'] + ' <b>' + self.translates['cheaper!'] + '</b>');
      $welcome.css('display', 'block');
      if (params.shiftLogo) {
        $('.logo').css('margin-left', coinWidth)
      }
      $text.magicText({minLineHeight: 1.2, verticalAlign: 'center'}).css('opacity', 1);
      $('.bg').after('<div class="sale2x-bg" style="height: ' + (tb.height + tb.top) + 'px; min-width: ' + coinWidth +
        'px; top: 0; left: 0; position: absolute; ' + params.bgCss + '">')
    }, 100);
  }

  this.applyMagicText = function ($el, css, newText) {
    setTimeout(function () {
      if (!css) {
        css = '';
      }
      $el.find('.mgctxt-wrapper > *').unwrap();
      if (!newText) {
        newText = $el.html();
      }
      $el.html(newText);
      $el.attr('style', $el.attr('style') + css);
      $el.magicText({minLineHeight: 1.1, verticalAlign: 'center'}).css('opacity', 1);
    }, 100);
  }

  this.replaceTextSimple = function (params) {
    params = $.extend({selector: '.welcome', css: '', bgCss: ''}, params);
    var $text = $(params.selector).css('font-size', 30);
    self.applyMagicText($text, params.css, self.translates['tokens'] + ' ' +
      self.translates['2 times'] + ' <b>' + self.translates['cheaper!'] + '</b>');
    $('.bg').after('<div class="sale2x-bg" style="position: absolute; ' + params.bgCss + '">')
  }


  this.getBanner = function (params) {
    params = $.extend({}, {width: 150, height: 100, lines: 3, className: ''}, params);
    if (params.lines === 3) {
      return '<div class="sale2x three-lines ' + params.className + '"><div class="sale2x-bg" /><div class="sale2x-wrp">' +
        '<div class="sale2x-line sale2x-tokens"><span>' +
        this.translates['tokens'] +
        '</span></div><div class="sale2x-line sale2x-times"><span>' +
        this.translates['2 times'] +
        '</span></div><div class="sale2x-line sale2x-cheaper"><span>' +
        this.translates['cheaper!'] +
        '</span></div></div></div>'
    } else {
      var tr2tc = this.translates['2 times'] + ' ' + this.translates['cheaper!'];
      return '<div class="sale2x two-lines ' + params.className + '"><div class="sale2x-bg" /><div class="sale2x-wrp"><div class="sale2x-line sale2x-tokens"><span>' +
        this.translates['tokens'] +
        '</span></div><div class="sale2x-line sale2x-times-cheaper"><span>' + tr2tc + '</span></div></div></div>'
    }
  }

  this.process = function () {
    this.type = this.params.fullscreen === 'on' ? 'fullscreen' : +this.params.slider ? 'slider' : this.params.bannerNumber ? 'targeted' : 'default';
    var size = this.params.width + 'x' + this.params.height;
    var subType = this.type === 'targeted' ? '' + this.params.bannerNumber : this.type === 'fullscreen' ? this.params.mode : '';
    var thumbCount = this.type === 'targeted' ? $('.video-group.active .slide-container').length : $('.bcdb_model_group_active .bcdb_model_item').length;
    var $anchor;
    switch (this.type) {
      case 'default':
        switch (size) {
          case '160x650':
            if (this.params.msize === 'min') {
              this.addAboveFooter({css: 'margin-top: 8px;'});
            } else {
              this.addOverThumb();
            }
            break;
          case '200x300':
            if (this.params.msize === 'min') {
              this.addOverThumb();
            } else {
              this.addUnderThumb({metaOver: true, height: 50});
            }
            break;
          case '240x480':
            if (this.params.msize === 'min') {
              this.addOverThumb();
            } else {
              this.addUnderThumb();
              if (thumbCount > 1) {
                $('.bcdb_model_item:nth-child(2)').hide();
                if (this.params.footer !== 'none') {
                  $('.bcdb_footer').show();
                }
              }
            }
            break;
          case '300x250':
            if (this.params.msize === 'min') {
              this.addAboveFooter({css: 'margin: auto; margin-top: 8px; width: 70%', height: 50}, {lines: 2});
              $('.bcdb_model_group_container').css('height', this.itemHeight);
            } else {
              this.addInnerThumb();
            }
            break;
          case '300x300':
            if (this.params.msize === 'min') {
              this.addAboveFooter({css: 'margin: auto; margin-top: 8px; width: 70%', height: 50}, {lines: 2});
              $('.bcdb_model_group_container').css('height', this.itemHeight);
            } else {
              this.addInnerThumb();
            }
            break;
          case '350x280':
            if (this.params.msize === 'min') {
              this.addAboveFooter({css: 'margin: auto; margin-top: 8px; width: 70%', height: 50}, {lines: 2});
              $('.bcdb_model_group_container').css('height', this.itemHeight);
            } else {
              this.addUnderThumb({metaOver: true});
            }
            break;
          case '400x600':
            if (this.params.msize === 'max') {
              this.addUnderThumb({height: 100}, {lines: 3});
            } else {
              this.addOverThumb();
            }
            break;
          case '700x90':
          case '728x90':
            this.addOverTwoThumb();
            break;
          case '720x200':
            this.addOverThumb({forceUnderThumbText: this.params.msize !== 'max'});
            break
          case '750x300':
            this.addOverThumb({forceUnderThumbText: this.params.msize !== 'min'});
            break
          default:
            if (thumbCount > 2) {
              this.addOverThumb();
            }
            break;
        }
        break;
      case 'fullscreen':
        switch (size) {
          case '160x650':
            switch (subType) {
              case 'mode1':
                this.addOverThumb();
                break;
              case 'mode2':
                this.addOverThumb();
                $('.modellink[data-newtext]').css({'visibility': 'hidden'});
                break;
              case 'mode3':
                $anchor = this.addOverThumb();
                if (!+this.params.mlink) {
                  $('.modelname[data-newtext]').hide();
                }
                $anchor.find('.modellang').hide();
                break;
              case 'mode4':
                $anchor = this.addOverThumb({mlinkTextOnly: true});
                break;
            }
            break;
          case '200x300':
            switch (subType) {
              case 'mode1':
                this.addUnderThumb({metaOver: true});
                break;
              case 'mode2':
                this.addUnderThumb();
                $('.modelname').css('bottom', 93);
                $('.modellink').css({top: 'auto', 'white-space': 'nowrap'});
                fitTextFontSize('.modellink');
                break;
              case 'mode3':
                this.addUnderThumb();
                break;
              case 'mode4':
                this.addUnderThumb({metaOver: true});
                break;
            }
            break;
          case '240x480':
            switch (subType) {
              case 'mode1':
                this.addInnerThumb();
                break;
              case 'mode2':
                var left = +this.params.header ? 110 : 2;
                this.addRelativeSelector({
                  anchor: '.bcdb',
                  css: 'top: 2px; right: 2px;height: 56px; left: ' + left + 'px;'
                }, {lines: 2});
                break;
              case 'mode3':
                this.addFlipTargeted({selector: '.bcdb_header, .bcdb_headersecond', css: 'height: 60px;'}, {lines: 3});
                break;
              case 'mode4':
                this.addFlipTargeted({selector: '.bcdb_header, .bcdb_headersecond', css: 'height: 50px; width: 100%'});
                break;
            }
            break;
          case '300x250':
            switch (subType) {
              case 'mode1':
                this.addInnerThumb({height: 42});
                if (!+this.params.header) {
                  $('.modellink').add('.modelname').css({'padding-left': '50%', 'box-sizing': 'border-box'});
                }
                break;
              case 'mode2':
                if (+this.params.header) {
                  this.addFlipTargeted({selector: '.bcdb_header, .bcdb_headersecond', css: 'top: -5px; width: 150px'});
                } else {
                  this.addInnerThumb({edge: 'nw', maxWidth: 150});
                }
                break;
              case 'mode3':
                this.addFlipTargeted({selector: '.bcdb_header, .bcdb_headersecond', css: 'height: 60px;'}, {lines: 3});
                break;
              case 'mode4':
                this.addFlipTargeted({selector: '.bcdb_header, .bcdb_headersecond', css: 'height: 50px; width: 100%'});
                break;
            }
            break;
          case '300x300':
            switch (subType) {
              case 'mode1':
                this.addInnerThumb();
                break;
              case 'mode2':
                this.addRelativeSelector({
                  anchor: '.bcdb',
                  css: 'bottom: 2px; left: 2px; height: 70px; right: 2px;'
                }, {lines: 2})
                break;
              case 'mode3':
                this.addFlipTargeted({selector: '.bcdb_header, .bcdb_headersecond', css: 'height: 60px;'}, {lines: 3});
                break;
              case 'mode4':
                this.addFlipTargeted({selector: '.bcdb_header, .bcdb_headersecond', css: 'height: 50px; width: 100%'});
                break;
            }
            break;
          case '350x280':
            switch (subType) {
              case 'mode1':
                this.addInnerThumb({height: 42});
                if (!+this.params.header) {
                  $('.modellink').add('.modelname').css({'padding-left': '50%', 'box-sizing': 'border-box'});
                }
                break;
              case 'mode2':
                if (+this.params.header) {
                  this.addFlipTargeted({selector: '.bcdb_header, .bcdb_headersecond', css: 'top: -5px; width: 150px'});
                } else {
                  this.addInnerThumb({edge: 'nw', maxWidth: 150});
                }
                break;
              case 'mode3':
                this.addFlipTargeted({selector: '.bcdb_header, .bcdb_headersecond', css: 'height: 60px;'}, {lines: 3});
                break;
              case 'mode4':
                this.addFlipTargeted({selector: '.bcdb_header, .bcdb_headersecond', css: 'height: 50px; width: 100%'});
                break;
            }
            break;
